#include <stdio.h>
#include <windows.h>
#include <math.h>
// підключення бібліотек
void result() // функція де проходять головні обчислення
{
    int n = 0; // створення змінних
    int check = 0;// //

    // Перевірка, чи не є введене число меншим за 1 та більшим за 10000
    do{
        check = 0; // змінна для виходу з циклу
        printf("\nВведіть довжину послідовності: ");
        scanf("%d", &n);
        if(n < 1 || n > 10000){ // умова перевірки
            check = 1; // змінна для продовження циклу
            printf("\nПослідовність n не може бути меншою за 1 та більшою за 10000\n"); // вивід повідомлення в консоль
        } 
    } while (check == 1); 

    // Початок логіки програми
    int dp[n + 1]; // масив int чисел
    dp[0] = 1;//
    dp[1] = 2;// початок масиву тепер| 1 | 2 | 3 |
    dp[2] = 3;//

    for (int i = 3; i <= n; i++){ // циклічно обчислюєм формулу
        dp[i] = (dp[i - 1] + dp[i - 2]) % 12345; // формула розрахунку
    }

    printf("\nКількість шуканих послідовностей: %d", dp[n]); // вивід результату

    system("pause"); //пауза програми
}

int main() // головна функція
{
    SetConsoleCP(1251);  // українське кодування
    SetConsoleOutputCP(1251);  // Налаштовує кодування виводу консолі на 1251 

    result();  // виклик функції result()

    exit(3); // примусове завершення програми 
}
