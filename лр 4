#include <stdio.h>
#include <windows.h>
// підключаєм потрібнінам бібліотеки

int main() {
  SetConsoleCP(1251); // для української мови
  SetConsoleOutputCP(1251); // теж
    int n = 0, count = 0; // створуєммо змінні для обчислення
    printf("Введіть число n: ");
    scanf("%d", &n); // скануємо число що ввів користувач
    while (n < 1 || n > 150) // перевірка на число n менше 150 та більше 1
    {
        printf("Введіть число 1 < n < 150: "); // повідомлення користувачу за неправельний ввід
        scanf("%d", &n);   } // скануємо число що ввів користувач
    for (int m = 1; m <= n; m++) { // умова циклу
        if (n % m == n / m) { // обчислення по формулі з теоретичних відомостей
            count++; // змінна в якій зберігається та додається результат
        }
    }
    printf("Кількість рівних дільників для числа %d: %d\n", n, count); // показ результату
    system("pause"); //пауза програми
    exit (3); // примусовий вихід
}
